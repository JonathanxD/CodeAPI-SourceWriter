package test;

public class InnerClass {

    protected String field = "XSD";

    public InnerClass() {
        new Inner(this).call();
    }

    protected InnerClass(String str) {
        System.out.println(str);
    }

    public void mm() {
        System.out.println("A");
    }

    public class Inner {

        private final InnerClass outer;
        public InnerClass a = new InnerClass("Hello");

        public Inner(InnerClass outer) {
            this.outer = outer;
        }

        protected String call() {
            System.out.println(this.outer.field);
            this.outer.mm();
            return "A";
        }
    }

}
